{
	"name": "quizzems-frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run scss && vite dev",
		"build": "npm run scss && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"scss": "sass src/scss/custom.scss static/app.css --no-source-map --style=compressed",
		"scss:watch": "sass --watch src/scss/custom.scss:static/app.css --style=expanded",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.89.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@supabase/auth-ui-svelte": "^0.2.9",
		"@supabase/supabase-js": "^2.50.0",
		"bootstrap": "^5.3.6",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"svelte-fa": "^4.0.4",
		"uuid": "^11.1.0"
	}
}
